config BOARD_AM301_T5D
	bool "Amlogic AM301 Board"
	select SOC_T5D
	help
		Enable Amlogic AM301 Board.

if BOARD_AM301_T5D

config FLASH_BASE_ADDRESS
	hex "flash base address"
	default 0x2000000

config PARTITION_BOOTLOADER_OFFSET
	hex "boot loader offset"
	default 0x0

config PARTITION_BOOTLOADER_SIZE
	hex "boot loader size"
	default 0x20000

endif
